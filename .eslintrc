{
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env":{
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended"
  ],
  "plugins": [
    "vue"
  ],
  "rules": {
    "semi": ["error", "always"]
  }
}
